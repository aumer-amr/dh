    "eslint": "^8.31.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "prettier": "^2.8.1",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "chalk": "^4.1.2",
    "chart.js": "3.9.1",
    "chartjs-chart-matrix": "^2.0.1",
    "chartjs-node-canvas": "^4.1.6",
    "chartjs-plugin-datalabels": "^2.2.0",
    "commander": "^10.0.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "winston": "^3.8.2"
